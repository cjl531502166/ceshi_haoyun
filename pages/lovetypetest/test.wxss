/* pages/lovetypetest/test.wxss */
@import '../futureresumetest/test.wxss';

.treeBg {
  background: url(data:image/png;base64,) repeat 0 0;
}

.tree {
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: 50% 30%;
  background-size: 115%;
}

.question-num {
  color: #e86651;
}

.refresh {
  background-image: url(data:image/png;base64,);
}

.midQuestion {
  width: 100%;
  background-size: 3%;
  background-image: url(data:image/png;base64,),
                      url(data:image/png;base64,);
  background-position: 2% center, 98% center;
  background-repeat: no-repeat, no-repeat;
  background-color: #fff;
  box-shadow: 0px 2px 5px 0 rgba(0, 0, 0, 0.3);
}

.questionTop {
  padding: 30rpx;
  margin: 30% auto 10% auto;
  color: #e86651;
  font-size: 32rpx;
  font-weight: 700;
  line-height: 40rpx;
  width: 90%;
  height: auto;
  transition: 0.5s;
}

.topicList .wx-li {
  border: 1px solid #e86651;
}

.topic-select {
  color: #e86651;
}

.nav_l {
  color: #e86651;
}

.listImage {
  width: 90%;
  margin: 0 auto;
}

.listImage image {
  width: 100%;
}

.topic-num-list-bg {
  margin: 0 auto;
  width: 90%;
  height: 30px;
  line-height: 30px;
  overflow: hidden;
  background: none;
}

.bgLight {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 28px 56px;
  background-position: 0 103%;
  color: #e86651;
}

.bgDeep {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 28px 56px;
  background-position: 0 -2%;
  color: #fff;
}

.nav_click {
  background-color: #e86651;
}
