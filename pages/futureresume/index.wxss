/* pages/futureresume/test.wxss */

.contentBg {
  height: 100%;
  background: url(data:image/png;base64,);
}

.center {
  text-align: center;
  display: block;
}

.my-title {
  font-size: 56rpx;
  margin: 0 auto 5%;
  color: #7f7f7f;
  padding-top: 5%;
}

.my-input {
  margin: 5% 15%;
  display: block;
  border: 1px solid #a5d8b9;
  border-radius: 8rpx;
  padding: 20rpx 40rpx;
  outline: none;
  font-size: 32rpx;
}

.my-btn {
  width: 70%;
  margin: 10% auto 0;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 50rpx;
  color: #fff;
  background: #00903b;
}
