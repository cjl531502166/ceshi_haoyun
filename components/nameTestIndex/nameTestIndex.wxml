<!--components/nameTestComp/testInexComp.wxml-->
<view class='contentBg'>
  <text class='center title'>{{title}}</text>
  <image class='sample-graph' mode='widthFix' src='{{sampleGraph}}'></image>
  <input bindinput='bindNameInput' class='text-input center' placeholder='请输入姓名'></input>
  <p class='hint-message center'>{{inputTips}}</p>
  <view bindtap='startTest' class="{{isNameEmpty?'bg_before':'bg_after'}} star-btn center">开始测算</view>
</view>